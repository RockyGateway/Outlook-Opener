{

  "description": "Opens outlook.live.com after clicking on it",
  "manifest_version": 2,
  "name": "Outlook Opener",
  "version": "1.0",
  "icons": {
    "48": "icons/outlook-48.png",
	"96": "icons/outlook-96.png"
  },

  "background": {
    "scripts": ["outlook.js"]
  },

  "browser_action": {
    "default_icon": "icons/outlook-32.png"
  }

}